{
  "name": "marlint",
  "version": "6.3.2",
  "description": "Javascript code linter for traveloka js styleguide",
  "license": "MIT",
  "repository": "traveloka/javascript",
  "author": {
    "name": "Traveloka",
    "url": "traveloka.com"
  },
  "bin": "cli.js",
  "scripts": {
    "test": "jest"
  },
  "files": [
    "*.js",
    "lib"
  ],
  "keywords": [
    "cli-app",
    "cli",
    "marlint",
    "code",
    "quality",
    "style",
    "lint",
    "linter",
    "jscs",
    "jshint",
    "jslint",
    "eslint",
    "validate",
    "code style",
    "standard",
    "strict",
    "check",
    "checker",
    "verify",
    "enforce",
    "hint",
    "simple"
  ],
  "dependencies": {
    "arrify": "^1.0.1",
    "babel-eslint": "^7.2.3",
    "eslint": "^4.5.0",
    "eslint-config-marlint": "^6.2.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-json": "^2.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "globby": "^6.0.0",
    "lodash.merge": "^4.6.0",
    "meow": "^3.7.0",
    "minimatch": "^3.0.4",
    "pkg-conf": "^2.0.0",
    "update-notifier": "^2.2.0"
  },
  "marlint": {
    "rules": {
      "no-console": 0,
      "no-empty": 0,
      "no-sync": 0
    }
  },
  "devDependencies": {
    "jest": "^20.0.4"
  }
}
